<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN" "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd" >
<taglib>
	<!-- 定义标签的版本库 -->
  <tlib-version>1.0</tlib-version>
  <!-- 定义jsp版本 -->
  <jsp-version>1.2</jsp-version>
  <short-name>MyElfunc</short-name>
  <uri>http://www.itheima/MyElfunc</uri>
  <tag>
    <name>URLEnc</name><!-- tag名字 -->
    <tag-class>com.itheima.util.MyElfunc</tag-class><!-- 对应的java类 -->
    <body-content>empty</body-content>
    <!--关于body-content 有三个值可选;empty：标签体必须为空;
    jsp：标签由其他jsp元素组成比如标签中含有<%=attributeName%>的jsp元素,
    那么此时body-content的值就是实际attributeName传入的值;
    tagdependent:有标签解释不带jsp转换（这个深入的含义不太了解）
    -->
    <attribute>
    	<name>cmsform</name><!-- tag的一个参数 -->
    	<required>true</required><!-- 参数名字 -->
    	<rtexprvalue>true</rtexprvalue><!-- 参数是否动态赋值 -->
    </attribute>
   	
  </tag>
  
  
  
</taglib>
