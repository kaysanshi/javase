package demo2;

public class TestString {

	public static void main(String[] args) {
		String s="we are students";
//	获取指定字符索引的位置用：对象名.indexOf(x);x为要索引的字符（对象）索因对象字符下标从0开始；
		System.out.println("字符中的s在哪个位置："+s.indexOf(s));
//		对象名.lastIndexOf(substr);subster为要搜索的对象,要索引出此对象的最后一次出现的位置
		System.out.println("字符s出现的最后一次的位置："+s.lastIndexOf(s));
		
		
	}

}
